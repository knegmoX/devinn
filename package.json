{
  "name": "devinn",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack -H 0.0.0.0",
    "build": "next build --turbopack",
    "start": "next start",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx",
    "lint:fix": "eslint . --ext .ts,.tsx,.js,.jsx --fix",
    "type-check": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:integration": "jest --config jest.integration.config.js",
    "test:e2e": "playwright test",
    "test:all": "npm run type-check && npm run lint && npm run test:coverage && npm run test:integration",
    "db:generate": "prisma generate",
    "db:migrate": "prisma migrate dev",
    "db:push": "prisma db push",
    "db:seed": "prisma db seed",
    "verify:env": "node -e \"console.log('Node:', process.version); console.log('Environment verified ✅')\"",
    "health:check": "node -e \"console.log('Health check passed ✅')\"",
    "qa:check": "npm run type-check && npm run lint",
    "pre-deploy": "npm run test:all && npm run build"
  },
